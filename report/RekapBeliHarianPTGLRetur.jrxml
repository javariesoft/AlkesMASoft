	<subDataset name="dataset1">
		<parameter name="idbeli" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[0]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
     RETURBELI."ID" AS RETURBELI_ID,
     RETURBELI."KODERETURBELI" AS RETURBELI_KODERETURBELI,
     RETURBELI."TANGGAL" AS RETURBELI_TANGGAL,
     SUM(JUMLAH * HARGA) AS JUMLAH,
     SUM(DISKON) AS DISKON,
     SUM(PPN) AS PPN,
     SUM(JUMLAH * HARGA-DISKON+PPN) AS TOTAL, sp.nama
FROM
     "PUBLIC"."RETURBELI" RETURBELI INNER JOIN "PUBLIC"."RETURBELIRINCI" RETURBELIRINCI ON RETURBELI."ID" = RETURBELIRINCI."IDRETURBELI"
INNER JOIN SUPPLIER sp on RETURBELI.IDSUPPLIER = sp.IDSUPPLIER
WHERE RETURBELI.IDPEMBELIAN=$P{idbeli}
GROUP BY
     RETURBELI_KODERETURBELI]]>
		</queryString>
		<field name="RETURBELI_ID" class="java.lang.Integer"/>
		<field name="RETURBELI_KODERETURBELI" class="java.lang.String"/>
		<field name="RETURBELI_TANGGAL" class="java.sql.Date"/>
		<field name="JUMLAH" class="java.lang.Double"/>
		<field name="DISKON" class="java.lang.Double"/>
		<field name="PPN" class="java.lang.Double"/>
		<field name="TOTAL" class="java.lang.Double"/>
		<field name="NAMA" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
	</subDataset>
