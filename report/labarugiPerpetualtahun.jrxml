	<queryString>
		<![CDATA[(SELECT
     NERACABANTU."KODEPERKIRAAN" AS NERACABANTU_KODEPERKIRAAN,
     NERACABANTU."NAMAPERKIRAAN" AS NERACABANTU_NAMAPERKIRAAN,
     NERACABANTU."GRUP" AS NERACABANTU_GRUP,
     NERACABANTU."TIPE" AS NERACABANTU_TIPE,
     GRUP.grup AS NAMAGRUP,getLaba($P{tahun}) as LABA,
     getDebet(NERACABANTU.KODEPERKIRAAN,$P{tahun}) as DEBET,
     getKredit(NERACABANTU.KODEPERKIRAAN,$P{tahun}) as KREDIT,
     getKredit(NERACABANTU.KODEPERKIRAAN,$P{tahun}) - getDebet(NERACABANTU.KODEPERKIRAAN,$P{tahun})
    as SALDO,VIEWSHPERKIRAAN.NAMAPERKIRAAN,1 as no
FROM
     "PUBLIC"."NERACABANTU" NERACABANTU
      INNER JOIN GRUP ON (NERACABANTU.GRUP=GRUP.KODE)
      INNER JOIN VIEWSHPERKIRAAN ON(left(NERACABANTU."KODEPERKIRAAN",3)=VIEWSHPERKIRAAN.KODEPERKIRAAN)
WHERE NERACABANTU."GRUP"=4 AND VIEWSHPERKIRAAN.KODEPERKIRAAN <> '412')
 union
(SELECT
     NERACABANTU."KODEPERKIRAAN" AS NERACABANTU_KODEPERKIRAAN,
     NERACABANTU."NAMAPERKIRAAN" AS NERACABANTU_NAMAPERKIRAAN,
     NERACABANTU."GRUP" AS NERACABANTU_GRUP,
     NERACABANTU."TIPE" AS NERACABANTU_TIPE,
     GRUP.grup AS NAMAGRUP,getLaba($P{tahun}) as LABA,
     getDebet(NERACABANTU.KODEPERKIRAAN,$P{tahun}) as DEBET,
     getKredit(NERACABANTU.KODEPERKIRAAN,$P{tahun}) as KREDIT,
     getDebet(NERACABANTU.KODEPERKIRAAN,$P{tahun}) - getKredit(NERACABANTU.KODEPERKIRAAN,$P{tahun})
    as SALDO,VIEWSHPERKIRAAN.NAMAPERKIRAAN,2 as no
FROM
     "PUBLIC"."NERACABANTU" NERACABANTU
      INNER JOIN GRUP ON (NERACABANTU.GRUP=GRUP.KODE)
      INNER JOIN VIEWSHPERKIRAAN ON(left(NERACABANTU."KODEPERKIRAAN",3)=VIEWSHPERKIRAAN.KODEPERKIRAAN)
WHERE NERACABANTU."GRUP"=5)
union
(select '51101','LABA KOTOR',8,'D','LABAKOTOR',getLaba($P{tahun}),0.0,0.0,(getKredit('411',$P{tahun}) - getDebet('411',$P{tahun}))-(getSaldoLR('5',$P{tahun})+getSaldoLR('61110',$P{tahun})),'LABAKOTOR',4)
 union
(
(SELECT
     NERACABANTU."KODEPERKIRAAN" AS NERACABANTU_KODEPERKIRAAN,
     NERACABANTU."NAMAPERKIRAAN" AS NERACABANTU_NAMAPERKIRAAN,
     NERACABANTU."GRUP" AS NERACABANTU_GRUP,
     NERACABANTU."TIPE" AS NERACABANTU_TIPE,
     GRUP.grup AS NAMAGRUP,getLaba($P{tahun}) as LABA,
     getDebet(NERACABANTU.KODEPERKIRAAN,$P{tahun}) as DEBET,
     getKredit(NERACABANTU.KODEPERKIRAAN,$P{tahun}) as KREDIT,
     - getKredit(NERACABANTU.KODEPERKIRAAN,$P{tahun}) + getDebet(NERACABANTU.KODEPERKIRAAN,$P{tahun})
    as SALDO,VIEWSHPERKIRAAN.NAMAPERKIRAAN,5
FROM
     "PUBLIC"."NERACABANTU" NERACABANTU INNER JOIN GRUP ON (NERACABANTU.GRUP=GRUP.KODE)
 INNER JOIN VIEWSHPERKIRAAN ON(left(NERACABANTU."KODEPERKIRAAN",3)=VIEWSHPERKIRAAN.KODEPERKIRAAN)
WHERE NERACABANTU."GRUP"=6 AND ((getDebet(NERACABANTU.KODEPERKIRAAN,$P{bulan})- getKredit(NERACABANTU.KODEPERKIRAAN,$P{tahun}) )>0) AND NERACABANTU."KODEPERKIRAAN" <> '61110'
)
)
 union
(select '62000','LABA USAHA',9,'D','LABAUSAHA',getLaba($P{tahun}),0.0,0.0,(getKredit('411',$P{tahun}) - getDebet('411',$P{tahun}))-(getSaldoLR('5',$P{tahun}))-(- getKredit('61',$P{tahun}) + getDebet('61',$P{tahun})),'LABAUSAHA',6)
union
(SELECT
     NERACABANTU."KODEPERKIRAAN" AS NERACABANTU_KODEPERKIRAAN,
     NERACABANTU."NAMAPERKIRAAN" AS NERACABANTU_NAMAPERKIRAAN,
     '10' AS NERACABANTU_GRUP,
     NERACABANTU."TIPE" AS NERACABANTU_TIPE,
     GRUP.grup AS NAMAGRUP,getLaba($P{tahun}) as LABA,
     getDebet(NERACABANTU.KODEPERKIRAAN,$P{tahun}) as DEBET,
     getKredit(NERACABANTU.KODEPERKIRAAN,$P{tahun}) as KREDIT,
     getKredit(NERACABANTU.KODEPERKIRAAN,$P{tahun}) - getDebet(NERACABANTU.KODEPERKIRAAN,$P{tahun})
    as SALDO,VIEWSHPERKIRAAN.NAMAPERKIRAAN,7
FROM
     "PUBLIC"."NERACABANTU" NERACABANTU
      INNER JOIN GRUP ON (NERACABANTU.GRUP=GRUP.KODE)
      INNER JOIN VIEWSHPERKIRAAN ON(left(NERACABANTU."KODEPERKIRAAN",3)=VIEWSHPERKIRAAN.KODEPERKIRAAN)
WHERE NERACABANTU."GRUP"=4 AND VIEWSHPERKIRAAN.KODEPERKIRAAN = '412')
order by 11,1]]>
