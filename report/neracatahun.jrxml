	<queryString>
		<![CDATA[(SELECT
     NERACABANTU."KODEPERKIRAAN" AS NERACABANTU_KODEPERKIRAAN,
     NERACABANTU."NAMAPERKIRAAN" AS NERACABANTU_NAMAPERKIRAAN,
     NERACABANTU."GRUP" AS NERACABANTU_GRUP,
     NERACABANTU."TIPE" AS NERACABANTU_TIPE,
     GRUP.grup AS NAMAGRUP,
getSaldoAwal(NERACABANTU.KODEPERKIRAAN,(CASEWHEN($P{bulan} - 1=0,12,$P{bulan}-1)),(CASEWHEN($P{bulan} - 1=0,$P{tahun}-1,$P{tahun}))) as SALDOAWAL,getDebet(NERACABANTU.KODEPERKIRAAN,$P{tahunlap}) as DEBET,getKredit(NERACABANTU.KODEPERKIRAAN, $P{tahunlap}) as KREDIT,
case NERACABANTU.GRUP
when 1 then
getSaldoAwal(NERACABANTU.KODEPERKIRAAN,(CASEWHEN($P{bulan} - 1=0,12,$P{bulan}-1)),(CASEWHEN($P{bulan} - 1=0,$P{tahun}-1,$P{tahun}))) + getDebet(NERACABANTU.KODEPERKIRAAN,$P{tahunlap}) - getKredit(NERACABANTU.KODEPERKIRAAN, $P{tahunlap})
when 2 then
getSaldoAwal(NERACABANTU.KODEPERKIRAAN,(CASEWHEN($P{bulan} - 1=0,12,$P{bulan}-1)),(CASEWHEN($P{bulan} - 1=0,$P{tahun}-1,$P{tahun}))) - getDebet(NERACABANTU.KODEPERKIRAAN,$P{tahunlap}) + getKredit(NERACABANTU.KODEPERKIRAAN,$P{tahunlap})
when 3 then
getSaldoAwal(NERACABANTU.KODEPERKIRAAN,(CASEWHEN($P{bulan} - 1=0,12,$P{bulan}-1)),(CASEWHEN($P{bulan} - 1=0,$P{tahun}-1,$P{tahun}))) - getDebet(NERACABANTU.KODEPERKIRAAN, $P{tahunlap}) + getKredit(NERACABANTU.KODEPERKIRAAN,$P{tahunlap})
end as SALDO,viewshperkiraan.namaperkiraan,viewhperkiraan.namaperkiraan as header,
casewhen(NERACABANTU."GRUP"=1,'AKTIVA','PASIVA') as kelompok

FROM
     "PUBLIC"."NERACABANTU" NERACABANTU INNER JOIN GRUP ON (NERACABANTU.GRUP=GRUP.KODE)
     inner join viewshperkiraan on (left(NERACABANTU."KODEPERKIRAAN",3)=viewshperkiraan.kodeperkiraan)
inner join viewhperkiraan on (left(NERACABANTU."KODEPERKIRAAN",2)=viewhperkiraan.kodeperkiraan)
WHERE (NERACABANTU."GRUP"=1 OR NERACABANTU."GRUP"=2 OR NERACABANTU."GRUP"=3) AND NERACABANTU."KODEPERKIRAAN" <> '12301' AND NERACABANTU."KODEPERKIRAAN" <> '12201' AND NERACABANTU."KODEPERKIRAAN" <> '12401' AND NERACABANTU."KODEPERKIRAAN" <> '31103' AND NERACABANTU."KODEPERKIRAAN" <> '31102' AND (getSaldoAwal(NERACABANTU.KODEPERKIRAAN,(CASEWHEN($P{bulan} - 1=0,12,$P{bulan}-1)),(CASEWHEN($P{bulan} - 1=0,$P{tahun}-1,$P{tahun})))<>0 OR getDebet(NERACABANTU.KODEPERKIRAAN,$P{tahunlap}) <> 0 OR getKredit(NERACABANTU.KODEPERKIRAAN,$P{tahunlap}) <> 0)
ORDER BY NERACABANTU."KODEPERKIRAAN")
union
(select '31103','LABA TAHUN BERJALAN',3,'D','MODAL',0.0,0.0,0.0,getLaba($P{tahunlap}),'MODAL','MODAL','PASIVA')
union
(select '31102','LABA DITAHAN',3,'D','MODAL',
getSaldoAwal('31102',(CASEWHEN($P{bulan} - 1=0,12,$P{bulan}-1)),(CASEWHEN($P{bulan} - 1=0,$P{tahun}-1,$P{tahun}))),
getDebet('31102',$P{tahunlap}),
getKredit('31102',$P{tahunlap}),
getSaldoAwal('31102',(CASEWHEN($P{bulan} - 1=0,12,$P{bulan}-1)),(CASEWHEN($P{bulan} - 1=0,$P{tahun}-1,$P{tahun}))) + getSaldoAwal('31103',(CASEWHEN($P{bulan} - 1=0,12,$P{bulan}-1)),(CASEWHEN($P{bulan} - 1=0,$P{tahun}-1,$P{tahun}))) -(getDebet('70001',$P{tahunlap})-getKredit('70001',$P{tahunlap})) - getDebet('31102',$P{tahunlap}) + getKredit('31102',$P{tahunlap}) ,
'MODAL',
'MODAL',
'PASIVA')
union
(select '12301','AKM. PENYUSUTAN KENDARAAN',1,'D','HARTA',getSaldoAwal('12301',(CASEWHEN($P{bulan} - 1=0,12,$P{bulan}-1)),(CASEWHEN($P{bulan} - 1=0,$P{tahun}-1,$P{tahun}))),getDebet('12301',$P{tahunlap}),getKredit('12301',$P{tahunlap}),-(getSaldoAwal('12301',(CASEWHEN($P{bulan} - 1=0,12,$P{bulan}-1)),(CASEWHEN($P{bulan} - 1=0,$P{tahun}-1,$P{tahun})))+getKredit('12301',$P{tahunlap})),'KENDARAAN','AKTIVA TETAP','AKTIVA')
union
(select '12201','AKM. PENYUSUTAN BANGUNAN',1,'D','HARTA',getSaldoAwal('12201',(CASEWHEN($P{bulan} - 1=0,12,$P{bulan}-1)),(CASEWHEN($P{bulan} - 1=0,$P{tahun}-1,$P{tahun}))),getDebet('12201',$P{tahunlap}),getKredit('12201',$P{tahunlap}),-(getSaldoAwal('12201',(CASEWHEN($P{bulan} - 1=0,12,$P{bulan}-1)),(CASEWHEN($P{bulan} - 1=0,$P{tahun}-1,$P{tahun})))+getKredit('12201',$P{tahunlap})),'BANGUNAN','AKTIVA TETAP','AKTIVA')
union
(select '12401','AKM. PENYUSUTAN INVENTARIS KANTOR',1,'D','HARTA',getSaldoAwal('12401',(CASEWHEN($P{bulan} - 1=0,12,$P{bulan}-1)),(CASEWHEN($P{bulan} - 1=0,$P{tahun}-1,$P{tahun}))),getDebet('12401',$P{bulan},$P{tahunlap}),getKredit('12401',$P{bulan},$P{tahunlap}),-(getSaldoAwal('12401',(CASEWHEN($P{bulan} - 1=0,12,$P{bulan}-1)),(CASEWHEN($P{bulan} - 1=0,$P{tahun}-1,$P{tahun})))+getKredit('12401',$P{tahunlap})),'INVENTARIS KANTOR','AKTIVA TETAP','AKTIVA') order by 3,1]]>
